<?xml version="1.0"?>
<launch>
  <arg name="current_world" default="wall_challenge.world"/>-->
  <!-- We resume the logic in empty_world.launch, changing only the name of the world to be launched-->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
  <!--use line for spawning world from .dae mesh in gz model folder-->
    <arg name="world_name" value="$(find autopilot)/worlds/$(arg current_world)"/>     
  </include>
  
  <!--drone flies in +x or -x direction-->
  <arg name="spawn_dir" default="3.14"/>
  <!-- Spawn simulated quadrotor uav -->
  <include file="$(find cvg_sim_gazebo)/launch/spawn_quadrotor.launch" >
    <arg name="model" value="$(find cvg_sim_gazebo)/urdf/quadrotor_sensors.urdf.xacro"/> 
    <arg name="y" value="0"/><!--adjust spawning position of drone-->
    <arg name="x" value="0"/>
    <arg name="Y" default="$(arg spawn_dir)"/>
  </include>
  
  
  <arg name="sloc" default="set_online"/>
  <arg name="logfile" default="/home/jay/autopilot_ws/src/autopilot/log.txt"/>
  
  <!-- start saving images in sloc saving location-->
  <node name="save_labelled_images_depth" pkg="autopilot" type="save_labelled_images_depth" output="screen" args="generated_set:='$(arg sloc)'" />
  <!-- start the heurist for control-->
  <arg name="steering_direction" default="left"/>
  <node name="teleop_autopilot_height" pkg="autopilot" type="teleop_autopilot_height" output="screen" args="s_dir:='$(arg steering_direction)'"/>
  
  <!-- keep evaluation for when the finish is reached-->
  <!--<node name="teleop_autopilot_evaluation_wall" pkg="teleop_autopilot" type="teleop_autopilot_evaluation_wall" output="screen" args="log:='$(arg logfile)'"/>-->
  <node name="teleop_autopilot_evaluation_wall" pkg="autopilot" type="teleop_autopilot_evaluation_wall" output="screen"/>
  <!-- Spawn human -->
  <!-- <include file="$(find cvg_sim_gazebo)/launch/spawn_human.launch" >
    <arg name="human" value="$(find cvg_sim_gazebo)/urdf/human.urdf.xacro"/> 
  </include>-->

 <!--<node name="teleop" pkg="teleop_twist_keyboard" type="teleop_twist_keyboard.py" output="screen"/>-->
<!-- <node name="rqt_gui" pkg="rqt_gui" type="rqt_gui" />-->
 <!-- JAY: Uncomment this line below if you want to launch your autopilot teleop imediately -->
  <!--<node name="teleop_autopilot_turning" pkg="teleop_autopilot" type="teleop_autopilot_turning" output="screen"/>-->
 
</launch>
